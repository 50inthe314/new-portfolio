#, php-format
msgid "POST id %d RECYCLED: '%s'"
msgstr "ID %d del mensaje Reciclado: '%s'"

#: auto-post-scheduler.php:516
#, php-format
msgid "%s POST id %d PUBLISHED : '%s'"
msgstr "%s ID del mensaje %d PUBLICADO: '%s'"

#: auto-post-scheduler.php:530
msgid "Unable to find eligible posts to publish."
msgstr "Incapaz de encontrar puestos elegibles para publicar."

#: auto-post-scheduler.php:551
#, php-format
msgid "Error reading log file (%s) Check that the file exists and is writable by WordPress"
msgstr ""
"Error archivo de registro de lectura (%s) Compruebe que el archivo existe y se puede escribir "
"con WordPress"
